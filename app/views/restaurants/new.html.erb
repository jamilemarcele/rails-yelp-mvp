<h1>Create a new restaurant</h1>

<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category, as: :select, collection: %w(chinese italian japanese french belgian) %>
  <%= f.input :phone_number %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
